class Solution {
    public int[] twoSum(int[] nums, int target, int[] lessthantarget) 
    {
        for(int i = 0; i < nums.length; i++)
        {
            for(int j = 0; j < nums.length; j++)
            {
                if 2 <= nums[i] && nums[i] < target
                {
                  nums[i] = lessthantarget[j];
                }
            }
        }
    }
}
